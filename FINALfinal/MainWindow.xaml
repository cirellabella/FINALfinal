<Window x:Class="FINALfinal.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FINALfinal"
        mc:Ignorable="d"
        Title="LoginPage" Height="450" Width="800">
    <Canvas Background="#FFA56464">
        <Label x:Name="lblUsername" Content="Username&#xD;&#xA;" Canvas.Top="134" Foreground="Black" FontSize="16" RenderTransformOrigin="0.469,0.447" Background="#00000000" Canvas.Left="196"/>
        <Label x:Name="lblPassword" Content="Password&#xD;&#xA;&#xA;" Canvas.Top="192" Foreground="Black" FontSize="16" HorizontalAlignment="Left" VerticalAlignment="Top" Background="#00000000" Canvas.Left="202"/>
        <TextBox x:Name="tbUsername" Canvas.Left="298" TextWrapping="Wrap" Canvas.Top="137" Width="204" FontSize="16" HorizontalAlignment="Left" VerticalAlignment="Top"/>
        <TextBox x:Name="tbPassword" Canvas.Left="298" TextWrapping="Wrap" Canvas.Top="202" Width="204" FontSize="16" HorizontalAlignment="Left" VerticalAlignment="Top"/>
        <Button x:Name="btnLogin" Content="Login" Canvas.Left="332" Canvas.Top="273" FontSize="16" Height="37" Width="136" Click="btnLogin_Click"/>
    </Canvas>
</Window>
